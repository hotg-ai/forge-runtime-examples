{
  "name": "@hotg-ai/forge-react-example",
  "description": "An example showing how the Forge frontend can be added to a React app",
  "private": true,
  "scripts": {
    "start": "parcel index.html  --no-source-maps",
    "build": "parcel build index.html --no-optimize"
  },
  "dependencies": {
    "@hotg-ai/forge": "^0.11.21",
    "@hotg-ai/rune-tflite": "^0.11.3",
    "@tensorflow/tfjs": "^3.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "staticFiles": {
    "staticPath": "node_modules/@tensorflow/tfjs-tflite/dist"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "parcel": "2.0.0-nightly.961",
    "parcel-reporter-static-files-copy": "^1.3.4"
  },
  "targets": {
    "default": {
      "sourceMap": {
        "inline": false,
        "inlineSources": false
      }
    }
  },
  "browserslist": [
    "defaults"
  ]
}
